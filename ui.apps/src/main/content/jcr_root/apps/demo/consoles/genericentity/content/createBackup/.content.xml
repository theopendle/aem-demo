<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:mixinTypes="[sling:VanityPath]"
          jcr:primaryType="nt:unstructured"
          jcr:title="New Backup"
          sling:resourceType="granite/ui/components/shell/propertiespage"
          backHref="/libs/granite/backup/content/admin.html"
          consoleId="granite-backup"
          formId="createBackupForm">
    <head jcr:primaryType="nt:unstructured">
        <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[coralui3,granite.ui.coral.foundation,granite.backup]"/>
    </head>
    <content
            granite:class="centered"
            granite:id="createBackupForm"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form"
            action="/libs/granite/backup/content/createBackup/content/items/backupform.html"
            method="post"
            style="vertical">
        <items jcr:primaryType="nt:unstructured">
            <backupform
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/backup/components/backupform"/>
        </items>
    </content>
    <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/text"
            text="New Backup"/>
</jcr:root>
